{
  "apps" : [{
    "name"      : "elect",
    "script"    : "elect.js",
    "instances" : "1",
    "exec_mode" : "cluster",
    "node_args": ["-r", "esm"],
    "cwd": "/sysu-score-elect",
    "watch"             : false,
    "log_date_format"   : "YYYY-MM-DD HH:mm:ss.SSS",
    "merge_logs"        : true,
    "error_file"        : "/var/log/elect/error.log",
    "out_file"          : "/var/log/elect/output.log",
    "max_memory_restart": "1G"
  }]
}